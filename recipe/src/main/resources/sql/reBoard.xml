<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="reboardns">

<select id="getTotal" parameterType="int" resultType="int">
	select count(*) from reboard where rnum = #{rnum}
</select>

<select id="list" parameterType="int" resultType="reboard">
	select * from reboard where rnum = #{rnum}
</select>

	
</mapper>